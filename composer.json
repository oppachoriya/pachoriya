{
	"description": "Ads Cottage an open source test project work",
	"name": "pachoriya/adscottage",
	"type": "project",
	"homepage": "http://www.adscottage.com",
	"license": "MIT",
	"support": {
		"forum": "http://forum.codeigniter.com/",
		"wiki": "https://github.com/bcit-ci/CodeIgniter/wiki",
		"irc": "irc://irc.freenode.net/codeigniter",
		"source": "https://github.com/bcit-ci/CodeIgniter"
	},
	"minimum-stability": "stable",
    "autoload": {
        "psr-4": {
		  "AdsCottage\\Entity\\": "application/models/Entities/",
		  "AdsCottage\\Repository\\": "application/models/Repositories/",
            "AdsCottage\\": "application/src/"
        }
    },
    "autoload-dev": {
		"psr-4": {
			"AdsCottage\\Tests\\": "application/tests/"
		}
    },
	"require": {
		"php": ">=5.2.4",
		"doctrine/common": "2.4.*",
        "doctrine/dbal": "2.4.*",
        "doctrine/orm": "2.4.*",
        "phpunit/phpunit": "4.3.*",
        "phpunit/dbunit": ">=1.2",
        "jms/serializer-bundle": "*"		
	},
	"require-dev": {
		"mikey179/vfsStream": "1.1.*"
	}
}